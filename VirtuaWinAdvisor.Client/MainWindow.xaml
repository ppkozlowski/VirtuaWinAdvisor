<Window x:Class="VirtuaWinAdvisor.Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VirtuaWinAdvisor.Client"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" SizeToContent="WidthAndHeight">
    <Grid>
        <StackPanel>
            <Button x:Name="previousDesktop" Height="40" Content="Previous Desktop" Click="previousDesktop_Click" />
            <Button x:Name="nextDesktop" Height="40" Content="Next Desktop" Click="nextDesktop_Click" />
        </StackPanel>
    </Grid>
</Window>
